<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
  <%include ../views/partials/_header%>
</head>
<body style="font-size:20px;">
    <h1>URLS</h1>
    <ul>
      <%for(var link in urls.urls) { %>
        <form method="POST" action="/urls/<%=link%>/delete">
        <%=link%> - <%=urls.urls[link]%> 
        <input type="submit" value="Delete">
        </form>
        <form method="POST" action="/urls/redirect/<%=link%>">
        <input type="submit" value="Edit">
        </form>
      <% } %>
    </ul>
</body>
</html>